# Azure OpenAI Configuration
AZURE_OPENAI_RESOURCE_NAME=your-azure-resource-name
AZURE_OPENAI_API_KEY=your-azure-openai-api-key
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# Optional: API Key for VEO3 endpoint protection
# Uncomment and set to enable API key authentication
# VEO3_API_KEY=your-secret-api-key

# Site URL for CORS and metadata
NEXT_PUBLIC_SITE_URL=https://superduperai.com

# Environment
NODE_ENV=production

# LangSmith Configuration for AI Tracing and Observability
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY="your-langsmith-api-key"
LANGCHAIN_PROJECT="super-landing"

# Google Analytics
NEXT_PUBLIC_GA_ID=your-ga-id 