  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@opennextjs/cloudflare": "1.0.0-beta.4",
    "@types/mdx": "^2.0.13",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20.17.32",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
